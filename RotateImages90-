javascript:(function(){var img=document.getElementsByTagName("img");for (var i=0;i<img.length;i++){img[i].setAttribute("style","rotation: -90deg;filter: progid:DXImageTransform.Microsoft.BasicImage(rotation=3);-moz-transform:rotate(-90deg);-webkit-transform:rotate(-90deg);-o-transform:rotate(-90deg);-ms-transform:rotate(-90deg);transform:rotate(-90deg);");}})();
